{
  "title": "模型定义",
  "type": "object",
  "format": "grid-strict",
  "required": [
    "__collection__",
    "_index",
    "_key",
    "createdate"
  ],
  "properties": {
    "__collection__": {
      "type": "string",
      "default": "collectionname"
    },
    "_index": {
      "type": "string",
      "default": "[{\"type\":\"hash\", \"fields\":[\"name\"],\"unique\":True}]"
    },
    "_key": {
      "type": "string",
      "default": "String(required=True)"
    },
    "createdate": {
      "type": "string",
      "default": "String(required=True, allow_none=False)"
    }
  }
}